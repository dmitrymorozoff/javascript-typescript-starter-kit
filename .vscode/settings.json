{
    "search.exclude": {
        ".git": true,
        ".vscode": true,
        "dist": true
    },
    "editor.renderWhitespace": "all",
    "editor.wordWrap": "on",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "all",
    "prettier.printWidth": 120,
    "tslint.jsEnable": true,
    "tslint.autoFixOnSave": true,
    "files.insertFinalNewline": true,
}
